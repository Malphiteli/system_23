<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.equipment.dao.EquipmentVersionMapper">

    <select id="findByType" resultType="com.example.equipment.entity.EquipmentVersionEntity">
        select * from equipment_version  where type = #{type}
    </select>

</mapper>
